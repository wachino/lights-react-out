{
  "name": "lights-react-out",
  "version": "1.0.0",
  "description": "Lights out, implemented with reactJS",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "start": "webpack-dev-server",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha \"test/**/*.test.js\" --recursive --timeout 15000 --exit;",
    "test:coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test:coverage-report": "npm run test:coverage; ./node_modules/opn-cli/cli.js coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wachino/lights-react-out.git"
  },
  "keywords": [
    "lights",
    "out",
    "reactjs",
    "game",
    "javascript"
  ],
  "author": "wachino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wachino/lights-react-out/issues"
  },
  "homepage": "https://github.com/wachino/lights-react-out#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semistandard": "^12.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "html-webpack-plugin": "^2.30.1",
    "mocha": "^5.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1",
    "opn-cli": "^3.1.0",
    "path": "^0.12.7",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "include": [
      "src/*"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  }
}
